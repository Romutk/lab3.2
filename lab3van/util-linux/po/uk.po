#: sys-utils/chcpu.c:244
msgid ""
"\n"
"Options:\n"
"  -h, --help                    print this help\n"
"  -e, --enable <cpu-list>       enable cpus\n"
"  -d, --disable <cpu-list>      disable cpus\n"
"  -c, --configure <cpu-list>    configure cpus\n"
"  -g, --deconfigure <cpu-list>  deconfigure cpus\n"
